{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Fireworks",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "C:/Pythons/test-install/python.exe",
			"program": "d:/fireworks.py",
			"cwd": "${workspaceRoot}",
			"env": {},
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "CPython: Tests",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/tests.py",
			"cwd": "${workspaceRoot}",
			"args": ["--pybase", "C:/Pythons/test-install"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "CPython: Tests --debug",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/tests.py",
			"cwd": "${workspaceRoot}",
			"args": ["--pybase", "C:/Pythons/test-install", "--debug", "cpp_exceptions"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "CPython: Bind Harfang",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/test.py",
			"cwd": "${workspaceRoot}",
			"args": ["--cpython", "--out", "d:/gs-fabgen-test/"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
