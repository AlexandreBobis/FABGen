{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "CPython: Tests",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/tests.py",
			"cwd": "${workspaceRoot}",
			"args": ["--pybase", "C:/Pythons/test-install", "--x64"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "CPython: Tests --debug",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/tests.py",
			"cwd": "${workspaceRoot}",
			"args": ["--pybase", "C:/Pythons/test-install", "--debug", "struct_default_comparison", "--x64"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "CPython: Bind Harfang",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/bind.py",
			"cwd": "${workspaceRoot}",
			"args": ["d:/gs-fabgen/harfang.py", "--cpython", "--out", "d:/gs-fabgen/"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "Lua53: Tests",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/tests.py",
			"cwd": "${workspaceRoot}",
			"args": ["--sdkbase", "D:/harfang/sdk", "--x64"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "Lua53: Tests --debug",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/tests.py",
			"cwd": "${workspaceRoot}",
			"args": ["--sdkbase", "D:/harfang/sdk", "--debug", "cpp_exceptions", "--x64"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "Lua53: Bind Harfang",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/bind.py",
			"cwd": "${workspaceRoot}",
			"args": ["d:/gs-fabgen/harfang.py", "--lua", "--out", "d:/gs-fabgen/"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "All: Bind Harfang",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/bind.py",
			"cwd": "${workspaceRoot}",
			"args": ["d:/gs-fabgen/harfang.py", "--lua", "--cpython", "--out", "d:/gs-fabgen/"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		}
	]
}