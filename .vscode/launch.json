{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Tests CPython",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/tests.py",
			"cwd": "${workspaceRoot}",
			"args": ["--pybase", "C:/Pythons/test-install"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "Tests CPython --debug",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/tests.py",
			"cwd": "${workspaceRoot}",
			"args": ["--pybase", "C:/Pythons/test-install", "--debug", "struct_bitfield_member_access"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "Tests Lua53 --debug",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/tests.py",
			"cwd": "${workspaceRoot}",
			"args": ["--luabase", "D:/lua53", "--debug", "struct_member_access"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "Bind Harfang",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/test.py",
			"cwd": "${workspaceRoot}",
			"args": ["--pybase", "C:/Pythons/test-install"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
